	</ITEM>	

</ETAPE>

<ETAPE id="6">

	<Libelle>Examen</Libelle>
	<ITEM type="RADIO" id="is_complication" >
		<Libelle>Complication pendant la PEC aux urgences ?</Libelle>
		<FromList>list:oui|non</FromList>
		<ShowItemsOnVal>
			<ListIdItems>list:complication</ListIdItems>
			<OnVal>const:oui</OnVal>
		</ShowItemsOnVal>
	</ITEM>
	
	<ITEM type="CHECK" id="complication" >
		<Libelle>Type de complication:</Libelle>
		<FromList>list:Hyperglycémie sup. 12 mmol|Hyperthermie sup. 37.5|Trouble de la vigilance|Convulsions|Autres</FromList>
		<ShowItemsOnVal>
			<ListIdItems>list:complication_autre</ListIdItems>
			<OnVal>const:Autres</OnVal>
		</ShowItemsOnVal>
		<ShowItemsOnVal>
			<ListIdItems>list:traitment_hyperglycemie</ListIdItems>
			<OnVal>const:Hyperglycémie sup. 12 mmol</OnVal>
		</ShowItemsOnVal>
		<ShowItemsOnVal>
			<ListIdItems>list:traitement_hyperthermie</ListIdItems>
			<OnVal>const:Hyperthermie sup. 37.5</OnVal>
		</ShowItemsOnVal>
        <ShowItemsOnVal>
			<ListIdItems>list:traitement_vigilance</ListIdItems>
			<OnVal>const:Trouble de la vigilance</OnVal>
		</ShowItemsOnVal>
        <ShowItemsOnVal>
			<ListIdItems>list:traitement_convulsions</ListIdItems>
			<OnVal>const:Convulsions</OnVal>
		</ShowItemsOnVal>
		<ShowItemsOnVal>
			<ListIdItems>list:traitement_autre</ListIdItems>
			<OnVal>const:Autres</OnVal>
		</ShowItemsOnVal>
	</ITEM>	
	
	<ITEM type="LONGTXT" id="complication_autre">
		<Libelle>Autre complication:</Libelle>
	</ITEM>	
	
	
	<ITEM type="CHECK" id="traitment_urgences">
		<Libelle>Traitement aux urgences</Libelle>
		<FromList>list:Aspirine|HBPM à dose préventive|HNF</FromList>
	</ITEM>

<ITEM type="CHECK" id="traitment_hyperglycemie" opt="y">
		<Libelle>Traitement de l'hyperglycémie</Libelle>
		<FromList>list:Insuline</FromList>
	</ITEM>

<ITEM type="CHECK" id="traitement_hyperthermie" opt="y">
		<Libelle>Traitement de l'hyperthermie</Libelle>
		<FromList>list:Paracetamol</FromList>
	</ITEM>

    <ITEM type="CHECK" id="traitement_vigilance" opt="y">
		<Libelle>Traitement de la vigilance</Libelle>
		<FromList>list:intubation oro-trachéale</FromList>
	</ITEM>

<ITEM type="CHECK" id="traitement_convulsions" opt="y">
		<Libelle>Traitement des convulsions</Libelle>
		<FromList>list:anticomitiaux</FromList>
	</ITEM>



<ITEM type="TXT" id="traitement_autre">
		<Libelle>Autre traitement:</Libelle>
	</ITEM>

</ETAPE>

<ETAPE id="7">
	<Libelle>Destination</Libelle>
	



	<ITEM type="CAL" id="jr_debut_transfert" opt="y">
		<Libelle>Début du transfert (jour)</Libelle>
		<From>formVar:date_pec_urgences</From>
	</ITEM> 

	<ITEM type="LISTE" id="hr_debut_transfert" opt="y">
