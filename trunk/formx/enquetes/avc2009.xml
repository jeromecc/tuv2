	</ITEM>
	
	<ITEM type="TXT" id="hopi_dest" opt="y">
		<Libelle>Hopital receveur</Libelle>
	</ITEM>	
	<ITEM type="LISTE" id="service_dest" opt="y">
		<Libelle>Service receveur</Libelle>
		<FromList>list:Médecine polyvalente|Neurologie|UNV|Neurochirurgie|Cardiologie|USI ou Réanimation|UHCD</FromList>
	</ITEM>	
	<ITEM type="RADIO" id="is_dc" >
